#:kivy 2.3.0

<RoundedButton@Button>:
    background_color: 0, 0, 0, 0
    color: 0.9, 0.9, 0.9, 1

    canvas.before:
        Color:
            rgba: (75/255, 82/255, 196/255, 1) if self.state == 'normal' else (60/255, 65/255, 156/255, 1)
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: [5,]